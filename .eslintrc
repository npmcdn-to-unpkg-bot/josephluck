{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"extends": "airbnb",
	"rules": {
		"indent": [2, "tab"],
		"react/jsx-filename-extension": [0],
		"react/jsx-indent-props": [0],
		"import/no-extraneous-dependencies": [0],
		"import/no-unresolved": [0],
		"react/jsx-indent": [2, 'tab']
	}
}